{
    "family": "Judge0Task",
    "containerDefinitions": [
        {
            "name": "kadane-judge0-server",
            "image": "905418014870.dkr.ecr.us-east-2.amazonaws.com/kadane/judge0:latest",
            "cpu": 256,
            "memory": 512,
            "portMappings": [
                {
                    "name": "judge0",
                    "containerPort": 2358,
                    "hostPort": 2358,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "privileged": false,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "judge0-server-logs",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "judge0-server"
                }
            }
        },
        {
            "name": "kadane-judge0-worker",
            "image": "905418014870.dkr.ecr.us-east-2.amazonaws.com/kadane/judge0:latest",
            "cpu": 256,
            "memory": 512,
            "essential": false,
            "privileged": false,
            "command": ["./scripts/workers"],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "judge0-worker-logs",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "judge0-worker"
                }
            }
        },
        {
            "name": "redis",
            "image": "905418014870.dkr.ecr.us-east-2.amazonaws.com/kadane/redis:latest",
            "cpu": 256,
            "memory": 512,
            "essential": true,
            "privileged": false,
            "command": ["redis-server", "--requirepass", "${REDIS_PASSWORD}", "--appendonly", "no"],
            "environment": [
                {
                    "name": "REDIS_PASSWORD",
                    "value": "${REDIS_PASSWORD}"
                }
            ],
            "healthCheck": {
                "command": [ "CMD-SHELL", "redis-cli -a ${REDIS_PASSWORD} ping" ],
                "retries": 3,
                "timeout": 5,
                "interval": 30
            },
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "redis-logs",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "redis"
                }
            }
        }
    ],
    "executionRoleArn": "arn:aws:iam::905418014870:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024",
    "memory": "2048"
}
